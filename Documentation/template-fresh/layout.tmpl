{{!include(/^js/.*/)}}
{{!include(/^css/.*/)}}
{{!include(favicon.ico)}}
<!DOCTYPE html>
<!--[if IE]><![endif]-->
<html>
  {{>partials/head}}
  <body data-spy="scroll" data-target="#affix" data-offset="110">
    <div class="flex-col">
      <header>
        {{^_disableNavbar}}
        {{>partials/navbar}}
        {{/_disableNavbar}}
        {{^_disableBreadcrumb}}
        {{>partials/breadcrumb}}
        {{/_disableBreadcrumb}}
      </header>
      <div class="flex-row flex-stretch no-overflow">
        {{>partials/toc}}
        <div class="flex-stretch flex-row scroll-y">
          <div class="flex-stretch">
            <div id="content-wrapper">
            {{!body}}
            </div>
          </div>
          {{>partials/affix}}
        </div>
      </div>
    </div>
    {{>partials/scripts}}
  </body>
</html>
